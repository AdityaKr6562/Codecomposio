from agent import crew
from prompts import BACKSTORY, DESCRIPTION, EXPECTED_OUTPUT, GOAL, ROLE


def main() -> None:
    """Run the agent."""

    crew.kickoff(
        inputs={
            "role": ROLE,
            "goal": GOAL,
            "backstory": BACKSTORY,
            "description": DESCRIPTION,
            "expected_output": EXPECTED_OUTPUT,
        }
    )


if __name__ == "__main__":
    main()
